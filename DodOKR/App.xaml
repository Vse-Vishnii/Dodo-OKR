<Application x:Class="DodOKR.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DodOKR"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/JoinStyles.xaml"/>
                <ResourceDictionary Source="Styles/ProgressBarStyle.xaml"/>
                <ResourceDictionary Source="Styles/GraphStyle.xaml"/>
                <ResourceDictionary Source="Styles/EdgeButtons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
